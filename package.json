{
  "name": "stonksjs",
  "version": "2.0.2",
  "description": "Reliable algotrading utilities written in node",
  "main": "lib/index.js",
  "files": [
    "lib/",
    "config/"
  ],
  "directories": {
    "lib": "lib/",
    "doc": "docs/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nielse63/stonksjs.git"
  },
  "homepage": "https://nielse63.github.io/stonksjs/",
  "bugs": {
    "url": "https://github.com/nielse63/stonksjs/issues"
  },
  "keywords": [
    "robinhood",
    "stocks",
    "screener",
    "finviz",
    "dividends",
    "finance"
  ],
  "engines": {
    "node": ">= 16.19.0"
  },
  "scripts": {
    "postinstall": "lerna bootstrap",
    "start": "node lib/index.js",
    "lint": "eslint --fix packages/ && prettier --write packages/",
    "test": "jest",
    "changelog": "auto-changelog",
    "release": ".bin/release",
    "docs": ".bin/docs",
    "create": ".bin/create-package"
  },
  "author": {
    "name": "Erik Nielsen",
    "email": "erik@312development.com",
    "url": "https://github.com/nielse63/"
  },
  "license": "ISC",
  "dependencies": {
    "date-fns": "^2.29.3",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "fs-extra": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "auto-changelog": "^2.4.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.3.1",
    "commander": "^9.5.0",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.19.0",
    "globby": "^11.0.1",
    "handlebars": "^4.7.6",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "jest-extended": "^0.11.5",
    "jsdoc-to-markdown": "^6.0.1",
    "lerna": "^3.22.1",
    "lint-staged": "^10.2.9",
    "lodash": "^4.17.20",
    "prettier": "^2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "jest --cache --onlyChanged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,html,css}": [
      "prettier --write"
    ]
  }
}
